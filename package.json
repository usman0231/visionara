{
  "name": "visionara",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "npm rebuild lightningcss || true; npm rebuild @tailwindcss/oxide || true; npm rebuild tailwindcss || true",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed --seed 001-roles.js && npx sequelize-cli db:seed --seed 002-settings.js && npx sequelize-cli db:seed --seed 003-sample-content.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.10.0",
    "@supabase/supabase-js": "^2.48.1",
    "@tanstack/react-query": "^5.62.7",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^2.0.1",
    "dotenv": "^17.2.2",
    "gsap": "^3.13.0",
    "jsonwebtoken": "^9.0.2",
    "lottie-web": "^5.13.0",
    "next": "^15.5.9",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.54.2",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.2",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.12.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "lightningcss": "^1.30.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
